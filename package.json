{
  "name": "ProyectosSolidarios",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios-setup": "sh ios-setup.sh",
    "test": "jest",
    "eslint": "eslint src/ *.js",
    "precommit": "npm run eslint",
    "lint-staged": "lint-staged",
    "bundle:ios": "react-native bundle --entry-file index.ios.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "react": "16.2.0",
    "react-native": "0.51.0",
    "react-native-firebase": "3.3.1",
    "react-native-fs": "2.11.17",
    "react-native-i18n": "^2.0.10",
    "react-native-progress": "^3.6.0",
    "react-native-vector-icons": "^4.3.0",
    "react-native-video": "^2.0.0",
    "react-navigation": "^1.0.0-beta.27",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.2.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-react-native": "^3.2.1",
    "husky": "^0.14.3",
    "jest": "20.0.4",
    "lint-staged": "^7.1.2",
    "prettier": "^1.13.2",
    "prettier-eslint": "^8.8.1",
    "prettierrc": "^0.0.0-5",
    "react-test-renderer": "16.0.0-alpha.12"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write --print-width 100 --tab-width 2 --single-quote --trailing-comma es5 --parser babylon"
    ]
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}
